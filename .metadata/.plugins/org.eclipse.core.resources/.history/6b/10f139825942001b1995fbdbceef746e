package demopack;

public class astrik {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	int k=1;
		for (int i=0;i<4;i++)
			
		{
			for(int j=0;j<4-1;j++)
			{
				System.out.print(j);
				System.out.println("\t");
				k++;
				}
			System.out.println();
			
			
		}
		
	}

}
